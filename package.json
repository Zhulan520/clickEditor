{
  "name": "click-insert-editor",
  "version": "0.1.7",
  "description": "可全程释放键盘的、点击插入式的编辑器",
  "keywords": [
    "react",
    "click",
    "insert",
    "editor",
    "clickEditor",
    "click-insert-editor"
  ],
  "homepage": "https://zhulan520.github.io/clickEditor",
  "repository": "https://github.com/Zhulan520/clickEditor",
  "license": "MIT",
  "main": "src/index.ts",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "father build",
    "build:watch": "father dev",
    "deploy": "gh-pages -d docs-dist",
    "dev": "dumi dev",
    "docs:build": "dumi build",
    "docs:preview": "dumi preview",
    "doctor": "father doctor",
    "lint": "npm run lint:es && npm run lint:css",
    "lint:css": "stylelint \"{src,test}/**/*.{css,less}\"",
    "lint:es": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install && dumi setup",
    "prepublishOnly": "father doctor && npm run build",
    "start": "npm run dev"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@codemirror/basic-setup": "^0.20.0",
    "@codemirror/language": "^0.20.2",
    "@codemirror/state": "^0.20.1",
    "@codemirror/view": "^0.20.7",
    "antd": "^5.23.4",
    "lodash": "^4.17.21",
    "mathjs": "^14.2.1"
  },
  "devDependencies": {
    "@codemirror/basic-setup": "^0.20.0",
    "@codemirror/language": "^0.20.2",
    "@codemirror/state": "^0.20.1",
    "@codemirror/view": "^0.20.7",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/lodash": "^4.17.15",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@umijs/lint": "^4.0.0",
    "dumi": "^2.4.13",
    "eslint": "^8.23.0",
    "father": "^4.1.0",
    "gh-pages": "^6.3.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prettier-plugin-packagejson": "^2.2.18",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "stylelint": "^14.9.1"
  },
  "peerDependencies": {
    "@codemirror/basic-setup": "^0.20.0",
    "@codemirror/language": "^0.20.2",
    "@codemirror/state": "^0.20.1",
    "@codemirror/view": "^0.20.7",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "authors": [
    "zhuqingyi"
  ],
  "overrides": {
    "@codemirror/language": "^0.20.2",
    "@codemirror/state": "^0.20.1",
    "@codemirror/view": "^0.20.7"
  }
}
